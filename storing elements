using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace storingelements
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] arr = new int[10];
            int i;

            Console.WriteLine("enter the 10 elements in an array : \n");
            for ( i = 0; i < 10; i++)
            {
                Console.WriteLine("elements - {0}", i);
                arr[i] = Convert.ToInt32(Console.ReadLine());
            }
            Console.WriteLine("Elements in an array: \n");
            for (i = 0; i < 10; i++)
            {
                Console.WriteLine("{0}", arr[i]);

            }
            Console.WriteLine("\n");
        }
    }
}
output;;
