using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace sumofelements
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int i,n,sum=0;
            int[] a = new int[100];

            Console.WriteLine("Enter the elements we need");
            n = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("input elements {0}",n);
            for ( i = 0; i < n; i++)
            {
                Console.WriteLine("elements-{0}",i);
                a[i] = Convert.ToInt32(Console.ReadLine());
            }
            for(i = 0; i < n; i++)
            {
                sum += a[i];
            }
            Console.WriteLine("sum of the elements : {0}",sum);
        }
    }
}
output;;
Enter the elements we need
5
input elements 5
elements-0
0
elements-1
1
elements-2
2
elements-3
3
elements-4
4
sum of the elements : 10
